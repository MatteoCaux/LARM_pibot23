### PATH TO EXECUTE
def path():
     # Initialize ROS node with ROS client
    rclpy.init()
    # Parcours Ã  executer
    print("mv1 go")
    res=True
    while res:
        res=move_metre(1)
        if res:
            res=move_degre(90)

